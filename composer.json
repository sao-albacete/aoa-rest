{
    "name": "aoa/aoa-rest",
    "description": "A RESTful API to connect with Anuario Ornitológico de Albacete Online database",
    "type": "project",
    "keywords": ["aoa","rest","api", "psr7", "sao"],
    "homepage": "http://anuario.albacete.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Víctor Cañizares Mata",
            "email": "victor.canizares.mata@gmail.com",
            "homepage": "http://viktorkhan.github.io/",
            "role": "Software engineer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "doctrine/orm": "2.5.*",
        "zendframework/zend-servicemanager": "3.0.*",
        "akrabat/rka-slim-zfsm-container": "^2.0",
        "apache/log4php": "^2.3",
        "psr/log": "^1.0",
        "vlucas/phpdotenv": "^2.2",
        "zendframework/zend-stdlib": "^3.0",
        "league/fractal": "^0.13.0",
        "acelaya/zsm-annotated-services": "^0.2.0",
        "vlucas/valitron": "^1.2",
        "cocur/slugify": "^2.1",
        "robmorgan/phinx": "^0.5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/prophecy": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Aoa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AoaTest\\": "test/Test"
        }
    }
}
